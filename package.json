{
  "name": "assets",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "author": "Falk Hoppe <falkhoppe81@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:css": "mkdir -p dist/styles && stylus styles/scs-commerce.main.styl -o dist/styles/scs-commerce.main.css",
    "watch:css": "mkdir -p dist/styles && stylus -w styles/scs-commerce.main.styl -o dist/styles/scs-commerce.main.css",
    "build:js": "node systemjs.build.js",
    "watch:js": "nodemon -e js --watch javascripts --watch systemjs.build.js --exec 'npm run build:js'",
    "watch:server": "nodemon -e js,pug --watch index.js --exec 'npm start'",
    "start": "PORT=$npm_package_config_port node index.js",
    "build": " npm-run-all --parallel build:*",
    "watch": " npm-run-all --parallel watch:*"
  },
  "config": {
    "port": 3003
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.15.0",
    "glob": "^7.1.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "standard": "^8.6.0",
    "stylus": "^0.54.5",
    "systemjs": "^0.20.7",
    "systemjs-builder": "^0.16.3",
    "systemjs-plugin-babel": "^0.0.21"
  },
  "dependencies": {
    "document-register-element": "^1.3.0",
    "es6-promise": "^4.0.5",
    "express": "^4.14.1",
    "morgan": "^1.8.1",
    "whatwg-fetch": "^2.0.2",
    "zepto": "^1.2.0"
  }
}
